{
    "name": "vscode-python-highlight-cell-splitter",
    "displayName": "Highlight Python Cell Splitter",
    "description": "highlight the splitter(title) of python cells",
    "version": "0.0.3",
    "publisher": "zhaouv",
    "repository": {
        "url": "https://github.com/zhaouv/vscode-python-highlight-cell-splitter.git"
    },
    "bugs": {
        "url": "https://github.com/zhaouv/vscode-python-highlight-cell-splitter/issues"
    },
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "python",
        "cell",
        "highlight"
    ],
    "contributes": {
        "grammars": [
            {
                "injectTo": [
                    "source.python"
                ],
                "scopeName": "comment.line.cell-splitter-injection.python",
                "path": "./syntaxes/python.cell.splitter.json"
            }
        ]
    }
}